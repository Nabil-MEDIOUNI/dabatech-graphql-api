{
  "spellright.language": ["en", "fr"],
  "spellright.documentTypes": ["markdown", "latex", "plaintext"],
  "eslint.options": {
    "extensions": [".js", ".ts"]
  },
  "eslint.validate": ["markdown", "mdx", "javascript", "typescript"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true
}
